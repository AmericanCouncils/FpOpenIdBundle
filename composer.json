{
    "name": "fp/openid-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 OpenID security extension",
    "keywords": ["Security", "OpenId", "Authentication"],
    "homepage": "https://github.com/formapro/FpOpenIdBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kotlyar Maksim",
            "email": "kotlyar.maksim@gmail.com"
        },
        {
            "name": "Community",
            "homepage": "https://github.com/formapro/FpOpenIdBundle/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "lightopenid/lightopenid": "1.0.*",
        "symfony/symfony": "2.1.*"
    },
    "repositories": [
        {
            "type": "package",
            "packagist": false,
            "package": {
                "name": "lightopenid/lightopenid",
                "version": "1.0.0",
                "dist": {
                    "url": "http://gitorious.org/lightopenid/lightopenid/archive-tarball/master",
                    "type": "tar.gz"
                }
            }
        }
    ],
    "autoload": {
        "psr-0": { "Fp\\OpenIdBundle": "../../" }
    },
    "target-dir": "Fp/OpenIdBundle"
}
